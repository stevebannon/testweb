{
"builders":
[
	{
		"type": "docker",
		"image": "ubuntu",
		"commit": "true",
		"login_server": "principal.el.nist.gov:5000"
	}
],

"provisioners":
[
	{
		"type": "shell",
		"inline": ["apt-get -y install curl"]
	},
	{
		"type": "chef-solo",
		"cookbook_paths": ["./vendor/cookbooks"],
		"run_list": ["recipe[testweb::default]"]
	}
],

"post-processors": [
	[
		{
			"type": "docker-tag",
			"repository": "principal.el.nist.gov:5000/testweb",
			"tag": "latest",
			"force": "true"
		},
		"docker-push"
	]
]
}
